# Copy this file to .env and update values.
# DO NOT commit your real .env to GitHub.

# VPN provider and credentials
VPN_SERVICE_PROVIDER=nordvpn
OPENVPN_USER=your_vpn_username_here
OPENVPN_PASSWORD=your_vpn_password_here

# General settings
TZ=UTC
LAN_CIDR=192.168.0.0/24
FIREWALL_OUTBOUND_SUBNET=0.0.0.0/0

# Ports
GLUETUN_WEBUI_PORT=30024
TORRENT_TCP_PORT=6881
TORRENT_UDP_PORT=6881

# User and group IDs (adjust if needed)
PUID=1000
PGID=1000
