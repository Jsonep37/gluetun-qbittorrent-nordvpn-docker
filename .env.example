# VPN provider credentials
VPN_SERVICE_PROVIDER=nordvpn
OPENVPN_USER=your_nordvpn_username
OPENVPN_PASSWORD=your_nordvpn_password

# Timezone (match your host system, e.g. Europe/London or America/New_York)
TZ=Etc/UTC

# LAN access (adjust to your network, e.g. 192.168.1.0/24)
LAN_CIDR=192.168.1.0/24

# User & group IDs (match your host system user for permissions)
PUID=1000
PGID=1000
